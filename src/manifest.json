{
    "manifest_version": 2,
    "name": "Music Party",
    "version": "0.0.5",
    "icons": {
        "128": "icon_128.png"
    },
    "content_security_policy":"script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com https://sdk.scdn.co/ https://apis.google.com 'unsafe-inline'; object-src 'self'",
    "background": {
        "page": "background.html",
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icon_19.png",
        "default_title": "Music Party",
        "default_popup": "popup.html"
    },
    "permissions": [
        "[http://*/*](http://*/*)",
        "[https://*/*](https://*/*)",
        "tabs",
        "identity",
        "[https://api.spotify.com/*](https://api.spotify.com/*)",
        "[https://accounts.spotify.com/*](https://accounts.spotify.com/*)",
        "[https://sdk.scdn.co/*](https://sdk.scdn.co/*)",
        "storage"
    ]
}